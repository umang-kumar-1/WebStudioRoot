{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "web-studio-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/webStudio/WebStudioWebPart.js",
          "manifest": "./src/webparts/webStudio/WebStudioWebPart.manifest.json"
        }
      ]
    },
    "image-management-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/imageManagement/ImageManagementWebPart.js",
          "manifest": "./src/webparts/imageManagement/ImageManagementWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "WebStudioWebPartStrings": "lib/webparts/webStudio/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "ImageManagementWebPartStrings": "lib/webparts/imageManagement/loc/{locale}.js"
  }
}